{
  "name": "okashoi/myapp-web",
  "authors": [
    {
      "name": "Okada Shohei",
      "email": "okashoicircus0@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "okashoi/myapp-task-component": "*@dev",
    "illuminate/support": "^6.0",
    "okashoi/myapp-database": "*@dev",
    "laravel/framework": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "MyApp\\Web\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Web\\": "./tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MyApp\\Web\\ServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../components/Tasks"
    },
    {
      "type": "path",
      "url": "../database"
    }
  ]
}
